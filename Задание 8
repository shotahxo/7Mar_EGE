k=0
for x1 in '123456789AB':
    for x2 in '0123456789AB':
        for x3 in '0123456789AB':
            for x4 in '0123456789AB':
                for x5 in '0123456789AB':
                    x=x1+x2+x3+x4+x5
                    if x.count('7')==1:
                        if x.count('9')+x.count('A')+x.count('B')<=3:
                            k+=1
print(k)
# Ответ: 67476
